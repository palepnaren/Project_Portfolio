<div id="contact" class="row">
    <div id="heading">
        <p>You can contact by sending me an email or by calling. </p>
    </div>
    <div id="form" class="col-md-offset-4 col-sm-offset-4 col-xs-offset-4">

        <% if(typeof(error)!=='undefined' && typeof(redirect)!=='undefined' && typeof(success)!=='undefined') {%>
        <% if(error !== ''){%>
        <div class="alert alert-danger">
            <a href="#" class="close" data-dismiss="alert" aria-label="close">×</a>
            <p><%= error%></p>
        </div>
        <%}%>
        <% if(success !== ''){%>
        <div class="alert alert-success">
            <a href="#" class="close" data-dismiss="alert" aria-label="close">×</a>
            <p><%= success%></p>
        </div>
        <%}%>
        <button type="submit" class="btn btn-default" id="redirect" value="Home" onClick="location.href='<%=redirect%>'">Home</button>
        <% } %>
        <form class="form-group <% if(typeof(prop)!=='undefined') {%> <%= prop%> <% }%>" method="post" action="/email">
            <div id="form-name" class="row">
                <label for="name" class="col-md-2 col-xs-2 col-sm-2 col-lg-2 col-form-label hidden-xs hidden-sm"> Name</label>
                <input type="text" class="form-control col-xs-10 col-lg-10 col-sm-10 col-md-10" id="name" name="full_name" placeholder="Enter name" >  
            </div>

            <div id="form-email" class="row">
                <label for="email" class="col-md-2 col-xs-2 col-sm-2 col-lg-2 col-form-label hidden-xs hidden-sm"> Email</label>
                <input type="email" class="form-control col-xs-10 col-lg-10 col-sm-10 col-md-10" id="email" name="email" placeholder="Enter email address" required>  
            </div>

            <div id="form-subject" class="row">
                <label for="subject" class="col-md-2 col-xs-2 col-sm-2 col-lg-2 col-form-label hidden-xs hidden-sm"> Subject</label>
                <input type="text" class="form-control col-xs-10 col-lg-10 col-sm-10 col-md-10" id="subject" name="subject" placeholder="Subject must" required>  
            </div>

            <div id="form-message" class="row">
                <label for="message" class="col-md-2 col-xs-2 col-sm-2 col-lg-2 col-form-label hidden-xs hidden-sm"> Message</label>
                <textarea type="text" class="form-control col-xs-10 col-lg-10 col-sm-10 col-md-10" id="message" name="message" required cols="10" rows="5" placeholder="Type your message.."></textarea>
            </div>

            <div id="form-buttons" class="row">
                <div id="submit" class="col-sm-6 col-md-6 col-lg-6 col-xs-6">
                    <button class="btn btn-md btn-success" value="submit" type="submit">Submit</button>
                </div>

                <div id="reset" class="col-sm-6 col-md-6 col-lg-6 col-xs-6">
                    <button class="btn btn-md btn-danger" value="reset" type="reset">Clear</button>
                </div>
            </div>
        </form>
    </div>
    <div id="number">
        <p>You can call me on &#64; &#40;469&#41; 626&#45;8767. </p>
    </div>
</div>


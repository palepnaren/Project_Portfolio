<div id="contact" class="container">
    <div class="row">
        <div id="msg" class="col-md-4 col-md-offset-4">
            <% if(typeof(error)!=='undefined' && typeof(redirect)!=='undefined' && typeof(success)!=='undefined') {%>
            <% if(error !== ''){%>
            <div class="alert alert-danger">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">×</a>
                <p><%= error%></p>
            </div>
            <%}%>
            <% if(success !== ''){%>
            <div class="alert alert-success">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">×</a>
                <p><%= success%></p>
            </div>
            <%}%>
            <button type="submit" class="btn btn-default" id="redirect" value="Home" onClick="location.href='<%=redirect%>'">Home</button>
            <% } %>
        </div>
        <div id="form" class="col-md-4 col-md-offset-4 panel panel-default <% if(typeof(prop)!=='undefined') {%> <%= prop%> <% }%>">

            <h1 align="center" class="margin-base-vertical">You can contact by sending me an email or by calling.</h1>

            <form class="margin-base-vertical" method="post" action="/email">
                <p class="input-group ">
                    <span class="input-group-addon"><span class="glyphicon glyphicon-user"></span></span>
                    <input type="text" class="form-control col-xs-10 col-lg-10 col-sm-10 col-md-10" id="name" name="full_name" placeholder="Enter name" > 
                </p>

                <p class="input-group ">
                    <span class="input-group-addon"><span class="glyphicon glyphicon-envelope"></span></span>
                    <input type="email" class="form-control col-xs-10 col-lg-10 col-sm-10 col-md-10" id="email" name="email" placeholder="Enter email address" required>
                </p>

                <p class="input-group ">
                    <span class="input-group-addon"><span class="glyphicon glyphicon-pencil"></span></span>
                    <input type="text" class="form-control col-xs-10 col-lg-10 col-sm-10 col-md-10" id="subject" name="subject" placeholder="Subject" required>
                </p>

                <p class="input-group ">
                    <span class="input-group-addon"><span class="glyphicon glyphicon-pencil"></span></span>
                    <textarea type="text" class="form-control col-xs-10 col-lg-10 col-sm-10 col-md-10" id="message" name="message" required cols="10" rows="5" placeholder="Type your message.."></textarea>
                </p>

                <br/>

                <p class="text-center">
                    <button type="submit" class="btn btn-success btn-md">Email</button>
                    <button class="btn btn-md btn-danger" value="reset" type="reset">Clear</button>
                </p>
            </form>
        </div>
    </div>
    <div id="number" class="<% if(typeof(prop)!=='undefined') {%> <%= prop%> <% }%>">
        <p>You can call me on &#64; &#40;469&#41; 626&#45;8767. </p>
    </div>
</div>

